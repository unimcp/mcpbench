[package]
name = "mcp-rust-server-tests"
version = "0.1.0"
edition = "2021"

[dependencies]
# SDK package will be added automatically
tokio = { version = "1.0", features = ["full"] }
axum = "0.7"
tower = "0.4"
tower-http = { version = "0.5", features = ["trace"] }
tracing = "0.1"
tracing-subscriber = { version = "0.3", features = ["env-filter"] }

[dev-dependencies]
mockall = "0.11.0"
tokio-test = "0.4" 